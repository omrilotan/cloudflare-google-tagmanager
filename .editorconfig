# EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = tab
tab_width = 2

[dist/*]
insert_final_newline = false

[*.svg]
insert_final_newline = false

[*.{yml,json}]
indent_style = space
